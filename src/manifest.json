{
  "manifest_version": 2,
  "name": "Covid-19-Search!",
  "version": "1.0.0",
  "description": "Get Covid-19 statistics for any country",
  "icons": {
    "16": "icons/16x16.png",
    "48": "icons/48x48.png",
    "128": "icons/128x128.png"
  },
  "permissions": [
    "<all_urls>"
  ],
  "browser_action": {
    "default_popup": "index.html"
  }
}